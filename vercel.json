{
  "buildCommand": "npm --prefix web install && npm --prefix web run build",
  "outputDirectory": "web/dist",
  "installCommand": "echo 'Skip root install'",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
